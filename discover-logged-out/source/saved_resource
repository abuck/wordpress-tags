	/* top shadow */
	/*background: #777 url(/wp-content/themes/h4/landing/../images/default.png) center -288px repeat-x; */
	color: #000;
	border-radius: 0 0 5px 5px;
	-moz-border-radius: 0 0 5px 5px;
	-webkit-border-radius: 0 0 5px 5px;
}
